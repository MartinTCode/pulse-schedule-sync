<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane
    fx:controller="com.pulse.frontend.ScheduleOverviewController"
    maxHeight="-Infinity"
    maxWidth="-Infinity"
    minHeight="-Infinity"
    minWidth="-Infinity"
    prefHeight="471.0"
    prefWidth="826.0"
    xmlns="http://javafx.com/javafx/23.0.1"
    xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <Label text="Schema från TimeEdit" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <BorderPane.margin>
            <Insets top="5.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <TableView fx:id="schemaTabell" prefHeight="392.0" prefWidth="826.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="kursKolumn" prefWidth="108.800048828125" text="Kurs" />
          <TableColumn fx:id="larareKolumn" prefWidth="135.199951171875" text="Lärare" />
            <TableColumn fx:id="datumKolumn" prefWidth="146.4000244140625" text="Datum" />
            <TableColumn fx:id="starttidKolumn" prefWidth="101.5999755859375" text="Starttid" />
            <TableColumn fx:id="sluttidKolumn" prefWidth="107.20001220703125" text="Sluttid" />
            <TableColumn fx:id="platsKolumn" prefWidth="120.00006103515625" text="Plats" />
            <TableColumn fx:id="andradKolumn" prefWidth="104.0" text="Ändrad" />
        </columns>
      </TableView>
   </center>
   <bottom>
      <HBox alignment="TOP_RIGHT" prefHeight="64.0" prefWidth="816.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="visaStatusSchema" text="Här skrivs statusmeddelanden ut">
               <HBox.margin>
                  <Insets right="200.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="tillbakaUrlKnapp" mnemonicParsing="false" prefHeight="31.0" text="Tillbaka" onAction="#onTillbakaUrlKnappClick">
               <HBox.margin>
                  <Insets right="30.0" top="15.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="andraHandelseKnapp" mnemonicParsing="false" prefHeight="31.0" prefWidth="166.0" text="Ändra markerad händelse" onAction="#onAndraHandelseKnappClick">
               <HBox.margin>
                  <Insets right="30.0" top="15.0" />
               </HBox.margin></Button>
            <Button fx:id="publiceraSchemaKnapp" mnemonicParsing="false" prefHeight="31.0" text="Publicera till Canvas">
               <HBox.margin>
                  <Insets top="15.0" />
               </HBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets right="10.0" />
         </BorderPane.margin>
      </HBox>
   </bottom>
</BorderPane>
